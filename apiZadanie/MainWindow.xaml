<Window x:Class="apiZadanie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:apiZadanie"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock Name="timezonetxt" Text="Podaj strefę czasową i kliknij aby zobaczyć aktualny czas" Margin="10,10,10,60" FontSize="16" TextAlignment="Center" VerticalAlignment="Top" TextWrapping="Wrap"/>

        
        <ComboBox x:Name="strefyLista" >
            
        </ComboBox>

        <Button Name="PrzyciskStrefa" Content="Zatwierdź" Width="150" Height="40" VerticalAlignment="Bottom" HorizontalAlignment="Center" Click="WybierzStrefe"/>
    </StackPanel>
</Window>
